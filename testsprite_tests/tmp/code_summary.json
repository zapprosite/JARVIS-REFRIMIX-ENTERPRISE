    "techStack": [
        "Python 3.12",
        "FastAPI",
        "LangGraph",
        "LangChain",
        "PostgreSQL",
        "Redis",
        "Docker",
        "Coolify"
    ],
    "architecture": "Microservices",
    "features": [
        {
            "name": "Orchestrator",
            "status": "Production-Ready",
            "description": "Central decision engine using LangGraph."
        },
        {
            "name": "Rate Limiting",
